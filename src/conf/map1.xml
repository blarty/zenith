<?xml version="1.0" encoding="UTF-8"?>
<channelMap name="firstMap" version="1.0">
    <channels>
        <channel name="routing"/>
        <channel name="filter1" listenerclass="org.jini.projects.zenith.messaging.test.items.cbr.CBRListener1"/>
        <channel name="filter2" listenerclass="org.jini.projects.zenith.messaging.test.items.cbr.CBRListener2"/>
        <channel name="filter3" listenerclass="org.jini.projects.zenith.messaging.test.items.cbr.CBRListener3"/>
        <channel name="controlCBR"/>
        <channel name="invalid" type="org.jini.projects.zenith.messaging.channels.InvalidMessageChannel"/>
    </channels>
    <endpoints>
        <endpoint id="CBR">
            <contentBasedRouter>
                <inputchannel id="routing"/>
                <controlchannel id="controlCBR"/>
                <invalidchannel id="invalid"/>
            </contentBasedRouter>
        </endpoint>
        
    </endpoints>
    <init>
        <send id="controlCBR" type="ObjectMessage">
            <header/>
            <content>
                <filterLink>
                    <linkchannel id="filter1"/>
                    <filter class="org.jini.projects.zenith.messaging.test.items.cbr.CBRFilter1"/>
                </filterLink>
            </content>
        </send>
        <send id="controlCBR" type="ObjectMessage">
            <header/>
            <content>
                <filterLink>
                    <linkchannel id="filter2"/>
                    <filter class="org.jini.projects.zenith.messaging.test.items.cbr.CBRFilter2"/>
                </filterLink>
            </content>
        </send>
        <send id="controlCBR" type="ObjectMessage">
            <header/>
            <content>
                <filterLink>
                    <linkchannel id="filter3"/>
                    <filter class="org.jini.projects.zenith.messaging.test.items.cbr.CBRFilter3"/>
                </filterLink>
            </content>
        </send>
    </init>
    <test>
        <send id="controlCBR" type="ObjectMessage">
            <header/>
            <content>
                <filterLink>
                    <linkchannel id="filter3"/>
                    <filter class="org.jini.projects.zenith.messaging.test.items.cbr.CBRFilter3"/>
                </filterLink>
            </content>
        </send>
    </test>
</channelMap>
